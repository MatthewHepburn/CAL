{
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double",
        "avoid-escape"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "$": false,
      "d3": false
    }
  }
}