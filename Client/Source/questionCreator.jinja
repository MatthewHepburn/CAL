<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>FSM Question Creator</title>
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Ek+Mukta:300" type="text/css">
    <link rel="stylesheet" href="{{ addresses.pureCSSAddress}}">
    <link rel="stylesheet" href="{{ addresses.cssAddress }}fsm.css">
    <link rel="stylesheet" href="{{ addresses.cssAddress }}questionCreator.css">
  </head>

  <body class="canvas"
  data-machinelist= '[{"nodes":[{"id":"B","x":100,"y":125,"isInit":true}],"links":[], "attributes": {"alphabet": ["a", "b"], "allowEpsilon": true}}]'
  data-question='{"type":"none"}'
  data-options='{"constrainRename":false, "responsiveResize": false}'
  data-iconaddress = 'img/icons/'
  data-pageid= "d156dfe8a5c746d88e7a97156e791365">
    <div class="maindiv">
    	<div class="questiontype"></div>
    	<div class="question"></div>
      <div class="questiondata"></div>
      <div class='buttondiv'></div>
      <div class='jsonout'></div>
      <div id="machine-div">
        <svg id="m1" width="500" height="300" viewBox="0 0 500 300" preserveAspectRatio="xMinYMin meet">
          <g class="links"></g>
          <g class="nodes"></g>
          <defs>
            <marker id="end-arrow" viewBox="0 -10 20 20" refX="7" markerWidth="5" markerHeight="5" orient="auto"><path d="M0,-10L20,0L0,10" fill="#000"></path></marker>
            <marker id="highlight-arrow" viewBox="0 -10 20 20" refX="7" markerWidth="5" markerHeight="5" orient="auto"><path d="M0,-10L20,0L0,10" fill="green"></path></marker>
          </defs>
        </svg>
      </div>
    </div>
    <script defer src="{{ addresses.jsAddress }}polyfill.min.js"></script>
    <script defer src="{{ addresses.d3Address }}"></script>
    <script defer src="{{ addresses.jsAddress }}victor.js"></script>
    <script defer src="{{ addresses.jsAddress }}model.js"></script>
    <script defer src="{{ addresses.jsAddress }}fsm.js"></script>
    <script defer src="{{ addresses.jsAddress }}questionCreator.js"></script>
  </body>
</html>