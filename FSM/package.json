{
  "name": "CAL-FSM",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npm install uglify-js -g; npm install uglifycss -g",
    "build": "python build.py",
    "babel": "babel Deploy -d Deploy",
    "uglifyjs": "uglifyjs  --screw-ie8 --compress"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double",
        "avoid-escape"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "eslint:recommended"
  }
}